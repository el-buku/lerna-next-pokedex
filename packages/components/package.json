    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@types/node": "11.13.0",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
